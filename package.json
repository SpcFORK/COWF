{
  "name": "cowf-parser",
  "version": "1.0.0",
  "description": "COWF Parser and CLI Tool",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node dist/index.js samples/sample.cowf -r -p -g --",
    "replit": "npm run build; npm run start; npm run test:cowf",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:cowf": "jest --selectProjects COWF",
    "test:artf": "jest --selectProjects Artf",
    "docs": "typedoc --out docs src",
    "test:performance": "ts-node tests/performanceTest.ts"
  },
  "keywords": [
    "cowf",
    "parser",
    "cli"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.108",
    "jest": "^27.1.5",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.22.18",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "chubml": "^1.0.2",
    "cowcst": "^1.1.30",
    "python-shell": "^3.0.1",
    "vm2": "^3.9.19"
  }
}
